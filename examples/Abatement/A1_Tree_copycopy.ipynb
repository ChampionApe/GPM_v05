   "source": [
    "db[m+'_e2t'] = DataBase_wheels.appmap(db.get(m+'_e2u'),DataBase_wheels.map_from_mi(u2t,n,nn),nn).unique()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "*Energyservice 2 aggregate input type 2 inidividual input type:*"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "MultiIndex([('CO2',           'K'),\n",
       "            ('CO2', 'electricity'),\n",
       "            ('CO2',        'inp3'),\n",
       "            ('CO2',         'oil')],\n",
       "           names=['z', 'n'])"
      ]
