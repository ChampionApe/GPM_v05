    "db[m+'_u'] = db.get(m+'_u_all').difference(pd.Index([x for y in output[m]['basetechs'].values() for x in y], name = n)) # technology goods, not baseline firms\n",
    "db['ai'] = output['ID']['Q2P'].levels[1].rename(n) # subset of aggregate goods."
